{
  "name": "map-gl-geolocation",
  "description": "A MapGL plugin to visualize a custom user geolocation",
  "version": "0.3.1",
  "author": "Thibaud Michel",
  "license": "MIT",
  "main": "dist/map-gl-geolocation.umd.js",
  "module": "dist/map-gl-geolocation.es.js",
  "types": "dist/map-gl-geolocation.d.ts",
  "style": "dist/map-gl-geolocation.css",
  "files": [
    "dist"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/map-gl-indoor/map-gl-geolocation.git"
  },
  "keywords": [
    "mapbox",
    "mapboxgl",
    "maplibre",
    "plugin",
    "geolocation",
    "indoor"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "build-dts": "dts-bundle-generator -o dist/map-gl-geolocation.d.ts src/index.ts",
    "prepublishOnly": "npm run build && npm run build-dts"
  },
  "peerDependencies": {
    "mapbox-gl": ">1.10.0"
  },
  "devDependencies": {
    "@turf/bearing": "^6.5.0",
    "@turf/destination": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "@types/mapbox-gl": "^1.13.2",
    "dts-bundle-generator": "^6.1.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "dependencies": {
    "map-gl-indoor": "^0.4.3",
    "mapbox-gl": "^1.10.0"
  },
  "exports": {
    ".": {
      "import": "./dist/map-gl-geolocation.es.js",
      "require": "./dist/map-gl-geolocation.umd.js"
    },
    "./dist/map-gl-geolocation.css": "./dist/map-gl-geolocation.css"
  }
}
